

  		<div class="row uc_data_container ucSwitchContentContainer" id="uc_tab_data_listuser" >
           <div class="ucSearchBar">
                <div class="ucSideBarMenuTrigger"><i class="ico-keypad"></i></div>
                 <div class="container-fluid">
                         
                         <div class="row">
                          <div class="col-lg-8 col-md-10">
                           <form class="form-inline">
                                <div class="ucSearchWrapper row">
                                  <div class="form-group col-md-4 col-sm-4 col-xs-4">
                                    <select class="form-control ucSearchSelect" id="ucVisitorSearchField">
                                        <option value="visitorData.name">Name</option>
                                        <option value="sessions.agentInfo.browser">Browser</option>
                                        <option value="sessions.agentInfo.os">Operating System</option>
                                        <option value="sessions.agentInfo.device">Device</option>
                                        <option value="sessions.geoLocationInfo.country">Country</option>
                                    </select>
                                    
                                  </div>
                                  <div class="form-group col-md-8 col-sm-8 col-xs-8 ">
                                    <input type="text" class="form-control" id="ucVisitorSearchValue">
