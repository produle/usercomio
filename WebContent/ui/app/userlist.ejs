                  <div class="col-md-12">

              
                        <div class="row">
                            <div class="col-md-12">
                            
                            </div></div>
                            
                        <div class="row">
                            <div class="col-md-12">
                                
                                

                                <div class="ucTable uc_table" id="uc_visitor_list">
                                    <div class="ucTableRow">
                                        <div class="ucTableHead uc_table_input_control">
                                            <input id="uc-all-user-select" type="checkbox"><label for="uc-all-user-select"></label>
                                        </div>
                                        <div class="ucTableHead ucUserListSortableColumn" data-sortColumn="visitorData.name">
                                            <div class="uc_table_header">
                                                Name <i class="fa fa-caret-up"></i><i class="fa fa-caret-down"></i>
                                            </div>
                                        </div>
                                        <div class="ucTableHead text-center ucVisitorListSession">
                                            <div class="uc_table_header text-center">
                                                Session
                                            </div>
                                        </div>
                                        <div class="ucTableHead ucUserListSortableColumn ucVisitorListToggleField ucVisitorListToggleField_browser" data-sortColumn="sessions.agentInfo.browser">
                                            <div class="uc_table_header text-center">
                                                Browser <i class="fa fa-caret-up"></i><i class="fa fa-caret-down"></i>
                                            </div>
                                        </div>
                                        <div class="ucTableHead ucUserListSortableColumn ucVisitorListToggleField ucVisitorListToggleField_os" data-sortColumn="sessions.agentInfo.os">
                                            <div class="uc_table_header text-center">
                                                Operating System <i class="fa fa-caret-up"></i><i class="fa fa-caret-down"></i>
                                            </div>
                                        </div>
                                        <div class="ucTableHead ucVisitorListToggleField ucVisitorListToggleField_device">
                                            <div class="uc_table_header text-center">
                                                Device
                                            </div>
                                        </div>
                                        <div class="ucTableHead ucUserListSortableColumn ucVisitorListToggleField ucVisitorListToggleField_country" data-sortColumn="sessions.geolocationinfo.country">
                                            <div class="uc_table_header text-center">
                                                Country <i class="fa fa-caret-up"></i><i class="fa fa-caret-down"></i>
                                            </div>
                                        </div>
                                        <div class="ucTableHead ucUserListSortableColumn ucVisitorListToggleField" rv-each-customfield="fieldList" rv-customfieldheader="customfield">
                                            <div class="uc_table_header text-center">
                                                <span class="ucVisitorListHeaderLabel"></span> <i class="fa fa-caret-up"></i><i class="fa fa-caret-down"></i>
                                            </div>
                                        </div>
                                        <div class="ucTableHead">
                                        </div>
                                    </div>
                                    <div class="ucTableRow" rv-each-visitor="list">
                                        <div class="ucTableCell uc_td_input_control">
                                            <input rv-visitorid="visitor" class="uc-user-select" type="checkbox"><label></label>
                                        </div>
                                        <div class="ucTableCell">
                                            <div class="ucUserBaseDetails ucVisitorPageTrigger" rv-data-visitorid="visitor._id">
                                               <div class="ucUserCurrentStatus">
                                                       <span rv-class-ucUserOnline="visitor.isVisitorOnline" rv-class-ucUserOffline="visitor.isVisitorOffline"></span>
                                                   <div class="ucUserPicWrapper" rv-profilepicture="visitor.visitorData">
                                                       <img class="ucUserProfileImage" src="" alt=""/>
                                                       <div class="ucUserProfileIntial"></div>

                                                   </div>

                                                   <div class="ucUserNameAndEmail">
                                                       <span class="ucUserName">{visitor.visitorData.name}</span>
                                                       <span class="ucUserEmail">{visitor.visitorData.email}</span>
                                                   </div>
                                               </div>

                                           </div>
                                        </div>
                                        <div class="ucTableCell text-center ucVisitorListSession" rv-sessioncount="visitor.sessions">

                                        </div>
                                        <div class="ucTableCell ucVisitorListToggleField ucVisitorListToggleField_browser">
                                            <div class="ucUserBrowserIcon" rv-latestbrowser="visitor.sessions"></div>
                                        </div>
                                        <div class="ucTableCell ucVisitorListToggleField ucVisitorListToggleField_os">
                                            <div class="ucUserOSIcon" rv-latestplatform="visitor.sessions"></div>
                                        </div>
                                        <div class="ucTableCell ucVisitorListToggleField ucVisitorListToggleField_device text-center">
                                            <div class="ucUserDeviceIcon desktop text-center"></div>
                                        </div>
                                        <div class="ucTableCell ucVisitorListToggleField ucVisitorListToggleField_country" rv-country="visitor.sessions">
                                        </div>
                                        <div class="ucTableCell ucVisitorListToggleField" rv-each-customfield="fieldList" rv-customfielddata="customfield" rv-customfieldtest="visitor.visitorData">
                                        </div>
                                        <div class="ucTableCell">
                                            <div class="dropdown ucUsersListMoreOptions">
                                              <span class="dropdown-toggle" type="button" id="ucUsersListMoreOptions" data-toggle="dropdown" aria-haspopup="true" aria-expanded="true">
                                                &#8226;&#8226;&#8226;
                                              </span>
                                              <ul class="dropdown-menu dropdown-menu-right" aria-labelledby="ucUsersListMoreOptions">
                                                <li><a href="#" class="ucSendMessageSingleTrigger">Send Message</a></li>
                                                <li><a href="#" class="ucVisitorPageTrigger" rv-data-visitorid="visitor._id">User Profile</a></li>
                                              </ul>
                                            </div>
                                        </div>
                                    </div>
                                </div>

                                <br/>

                                <div id="ucVisitorListAjaxLoader" class='ucAjaxLoader'><div></div></div>

                                <br/>

                                <div id="ucVisitorListPlaceholder" rv-show="display">
                                    <div>
                                        To track visitors, copy the tracking code to your application. You can get the tracking code by clicking app list dropdown at the top-right corner and then the settings icon near your app name.
                                    </div>
                                </div>


                            </div>
                        </div>
                    </div>

                </div>
